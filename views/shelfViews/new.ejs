  <% include ../partials/header.ejs %>

	<fieldset>
		<legend>New Shelf: </legend>
		<form action="/shelves" method="POST">
			Title: <input type="text" name="title"><br><br>
			Albums: <br>
			<% allAlbums.forEach(oneAlbum => { %>
				<div>
					<input type="checkbox" id="<%=oneAlbum%>CheckBox" name="album" value="<%=oneAlbum.id%>">
					<label for="<%=oneAlbum%>CheckBox">Album Title: <%=oneAlbum.title%> | Album Artist: <%=oneAlbum.artist%></label>
				</div>
			<% }) %>
			<br>
			<button type="submit">Create Shelf</button>
		</form>
	</fieldset>

</body>
</html>